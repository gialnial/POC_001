@using TemplatesDoctor = Sitecore.Feature.Doctor.Templates
@using Sitecore.Foundation.Alerts
@using Sitecore.Foundation.Alerts.Extensions
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Foundation.SitecoreExtensions.Repositories
@model Sitecore.Foundation.SitecoreExtensions.Rendering.QueryableDatasourceRenderingModel

@if (Model?.Items == null || !Model.Items.All(i => i.IsDerived(TemplatesDoctor.Doctor.ID)))
{
  @Html.PageEditorError(AlertTexts.InvalidDataSourceTemplate(TemplatesDoctor.Doctor.ID), AlertTexts.InvalidDataSourceTemplateFriendlyMessage, Model.Item?.ID, Model.Rendering.Item?.ID)
  return;
}
@Html.Partial("_DoctorDetails",Model) 
